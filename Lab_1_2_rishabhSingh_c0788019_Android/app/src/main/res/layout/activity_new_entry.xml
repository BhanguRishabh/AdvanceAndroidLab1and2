<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".newEntry">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".newEntry">

        <EditText
            android:id="@+id/etxtPrice"
            android:layout_width="108dp"
            android:layout_height="40dp"
            android:layout_marginStart="62dp"
            android:layout_marginTop="49dp"
            android:ems="10"
            android:inputType="numberDecimal"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/etxtName" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="141dp"
            android:layout_height="33dp"
            android:layout_marginStart="46dp"
            android:layout_marginTop="28dp"
            android:text="LONGITUDE"
            android:textAlignment="center"
            android:textColor="#010C2C"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView11" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="146dp"
            android:layout_height="32dp"
            android:layout_marginStart="41dp"
            android:layout_marginTop="45dp"
            android:text="LATITUDE"
            android:textAlignment="center"
            android:textColor="#010C2C"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="126dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:text="NAME"
            android:textAlignment="center"
            android:textColor="#010C2C"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="126dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="56dp"
            android:text="PRICE"
            android:textAlignment="center"
            android:textColor="#010C2C"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="198dp"
            android:layout_height="47dp"
            android:layout_marginStart="98dp"
            android:layout_marginTop="12dp"
            android:layout_marginEnd="115dp"
            android:text="ENTER DETAIL"
            android:textAlignment="center"
            android:textColor="#ED0808"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <EditText
            android:id="@+id/etxtName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="36dp"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="PRODUCT"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <EditText
            android:id="@+id/etxtDes"
            android:layout_width="329dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="84dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="DESCRIBE IT..."
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etxtPrice" />

        <EditText
            android:id="@+id/etxtLong"
            android:layout_width="108dp"
            android:layout_height="40dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="20dp"
            android:ems="10"
            android:inputType="numberDecimal"
            app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/etxtLat" />

        <EditText
            android:id="@+id/etxtLat"
            android:layout_width="108dp"
            android:layout_height="40dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="45dp"
            android:ems="10"
            android:inputType="numberDecimal"
            app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="296dp"
            android:layout_height="35dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="32dp"
            android:text="LOCATION CO-ORDINATES"
            android:textAlignment="center"
            android:textColor="#010C2C"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etxtDes" />

        <ImageButton
            android:id="@+id/btSave"
            android:layout_width="63dp"
            android:layout_height="55dp"
            android:layout_marginStart="140dp"
            android:layout_marginTop="28dp"
            android:backgroundTint="#8A6060"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:srcCompat="@android:drawable/ic_menu_save" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="153dp"
            android:layout_height="30dp"
            android:layout_marginStart="76dp"
            android:layout_marginTop="32dp"
            android:text="DESCRIPTION"
            android:textAlignment="center"
            android:textColor="#010C2C"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/etxtPrice" />

        <Button
            android:id="@+id/btBacktoMain"
            android:layout_width="70dp"
            android:layout_height="38dp"
            android:layout_marginStart="3dp"
            android:layout_marginTop="11dp"
            android:backgroundTint="#F4F1F1"
            android:text="BACK"
            android:textColor="#050A5A"
            android:textColorHint="#38219A"
            android:textSize="10sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>